CHIP DisplayCounter201860153 {
    IN  inc, reset;
    OUT a, b, c, d, e, f, g;

    PARTS:
    Bit(in=in0, load=load, out=out0);
    Bit(in=in1, load=load, out=out1);
    Bit(in=in2, load=load, out=out2);
    Bit(in=in3, load=load, out=out3);

    Decoder201860153(
        A=out0,
        B=out1,
        C=out2,
        D=out3,
        a=a, b=b, c=c, d=d, e=e, f=f, g=g
    );
}




